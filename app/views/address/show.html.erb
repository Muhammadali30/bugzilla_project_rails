<h1>Students</h1>
  <%= link_to "Back To all Students","/address#index" %><br>
<table class="table table-hover">
<thead>
<th>Student ID</th>
<th>Student Name</th>
<th>Student Email</th>
<th>Student Address</th>
<th>Delete</th>
</thead>
<tbody>
<% @address.students.each do |stu| %>

<tr>
<td><%=stu.id%></td>
<td><%=stu.name%></td>
<td><%=stu.email%></td>
<td><%=@address.city%></td>
<td><%= link_to "Delete",student_path(stu), data: {
                    turbo_method: :delete,
                    turbo_confirm: "Are you sure?"
                  }%></td>
</tr>
<%end %>
</tbody>
</table>